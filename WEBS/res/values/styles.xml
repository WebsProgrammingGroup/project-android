<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
	<style name="textStyle01">
	    <item name="android:textStyle">normal</item>
	    <item name="android:textSize">15sp</item>
	</style>
	<style name="f09_AppSettingLayout">
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">40dp</item>
	    <item name="android:paddingRight">7dp</item>
	    <item name="android:paddingLeft">7dp</item>
	    <item name="android:paddingTop">5dp</item>
	    <item name="android:paddingBottom">5dp</item>
	    <item name="android:background">#ffffff</item>
	    <item name="android:clickable">true</item>
	    <item name="android:background">@drawable/sel_layout1</item>
	</style>
	<style name="TitleLayout">
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:paddingRight">7dp</item>
	    <item name="android:paddingLeft">7dp</item>
	    <item name="android:paddingTop">2dp</item>
	    <item name="android:paddingBottom">2dp</item>
	    <item name="android:background">#ffffff</item>
	    <item name="android:clickable">true</item>
	</style>
	<style name="f05_calendar_title">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:gravity">right</item>
	    <item name="android:textSize">15sp</item>
	    <item name="android:textColor">#ffffff</item>
	    <item name="android:paddingRight">2dp</item>
	</style>
	<style name="f10_team_text">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:textSize">15sp</item>
	    <item name="android:textColor">#ffffff</item>
	</style>
	<style name="f10_team_icon">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:textSize">23sp</item>
	    <item name="android:textColor">#ffffff</item>
	</style>
</resources>
